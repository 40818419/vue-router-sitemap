  "dependencies": {
    "sitemap": "1.13.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "eslint": "4.19.1",
    "eslint-plugin-vue": "4.4.0",
    "jest": "22.4.3"
  },
  "peerDependencies": {
    "vue-router": "3.0.1"
