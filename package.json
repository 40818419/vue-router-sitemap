{
  "name": "vue-router-sitemap",
  "version": "1.0.0",
  "description": "Module to generate a sitemap for vue-router configuration",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --silent --runInBand",
    "test:coverage": "jest --silent --coverage",
    "build": "babel ./lib --out-dir=./dist --presets env",
    "lint": "eslint --fix --ignore-path .gitignore ."
  },
  "repository": {
